<?xml version="1.0" encoding="ASCII"?>
<openCypher:SinglePartQuery
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:openCypher="http://www.slizaa.org/neo4j/opencypher/OpenCypher">
  <readingClauses
      xsi:type="openCypher:Match">
    <pattern>
      <patterns>
        <var/>
        <part
            xsi:type="openCypher:PatternElement">
          <nodepattern>
            <variable
                name="B"/>
          </nodepattern>
        </part>
      </patterns>
    </pattern>
  </readingClauses>
  <return>
    <body>
      <returnItems>
        <items>
          <expression
              xsi:type="openCypher:VariableRef"
              variableRef="//@readingClauses.0/@pattern/@patterns.0/@part/@nodepattern/@variable"/>
        </items>
      </returnItems>
    </body>
  </return>
</openCypher:SinglePartQuery>
